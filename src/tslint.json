{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],


    "no-use-before-declare": false,
    "ban-types": false,
    "no-bitwise": false,
    "callable-types": false,
    "prefer-for-of": false,
    "no-unnecessary-initializer": false,
    "variable-name": false
  }
}
